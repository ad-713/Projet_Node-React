{
  "compilerOptions": {
    "target": "ESNext", // or "ES6"
    "module": "ESNext", // or "ES6"
    "moduleResolution": "node16", // or "nodenext"
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  
}
